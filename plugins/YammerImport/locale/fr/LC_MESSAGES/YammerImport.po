msgstr "Secret de l'utilisateur :"

#: lib/yammerapikeyform.php:98
msgid "Save"
msgstr "Sauvegarder"

#: lib/yammerapikeyform.php:98
msgid "Save these consumer keys"
msgstr "Enregistrer ces clés utilisateur"

